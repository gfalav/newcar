<h1>Listing actividads</h1>

<table>
  <tr>
    <th>Cuenta</th>
    <th>Fdesde</th>
    <th>Fhasta</th>
    <th>Cantidad</th>
    <th>Hscar</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @actividads.each do |actividad| %>
  <tr>
    <td><%= actividad.cuenta_id %></td>
    <td><%= actividad.fdesde %></td>
    <td><%= actividad.fhasta %></td>
    <td><%= actividad.cantidad %></td>
    <td><%= actividad.hscar %></td>
    <td><%= link_to 'Show', actividad %></td>
    <td><%= link_to 'Edit', edit_actividad_path(actividad) %></td>
    <td><%= link_to 'Destroy', actividad, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Actividad', new_actividad_path %>
